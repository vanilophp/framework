{
    "name": "vanilo/checkout",
    "description": "Vanilo Checkout Module",
    "type": "library",
    "license": "MIT",
    "keywords": ["checkout", "ecommerce", "vanilo", "artkonekt", "laravel"],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "support": {
        "source": "https://github.com/vanilophp/checkout"
    },
    "authors": [
        {
            "name": "Attila Fulop",
            "homepage": "https://github.com/fulopattila122"
        }
    ],
    "require": {
        "php": "^7.2",
        "konekt/concord": "~1.5",
        "vanilo/contracts": "~2.0@dev"
    },
    "require-dev": {
        "phpunit/phpunit": "~8.0|~9.0",
        "orchestra/testbench": "~4.0|~5.0",
        "konekt/enum": "~2.1",
        "vanilo/support": "~2.0@dev"
    },
    "autoload": {
        "psr-4": { "Vanilo\\Checkout\\": "src/" }
    },
    "autoload-dev": {
        "psr-4": {
            "Vanilo\\Checkout\\Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.0.x-dev"
        }
    }
}

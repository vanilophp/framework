{
    "name": "vanilo/master-product",
    "description": "Vanilo Master Product Module",
    "type": "library",
    "license": "MIT",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "keywords": ["products", "variants", "ecommerce", "vanilo", "laravel"],
    "support": {
        "issues": "https://github.com/vanilophp/framework/issues",
        "source": "https://github.com/vanilophp/master-product"
    },
    "authors": [
        {
            "name": "Attila Fulop",
            "homepage": "https://github.com/fulopattila122"
        }
    ],
    "require": {
        "php": "^8.4",
        "cviebrock/eloquent-sluggable": "^12.0",
        "konekt/concord": "^1.15",
        "konekt/enum": "^4.2",
        "konekt/enum-eloquent": "^1.9",
        "laravel/framework": "^12.50",
        "vanilo/contracts": "^6.0",
        "vanilo/product": "^6.0",
        "vanilo/support": "^6.0"
    },
    "suggest": {
        "vanilo/properties": "Allows establishing variants based on properties like color, size, etc"
    },
    "require-dev": {
        "phpunit/phpunit": "^11.0|^12.0",
        "orchestra/testbench": "^10.0",
        "vanilo/properties": "^6.0"
    },
    "autoload": {
        "psr-4": { "Vanilo\\MasterProduct\\": "" }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "6.0.x-dev"
        }
    }
}

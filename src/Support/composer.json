{
    "name": "vanilo/support",
    "description": "Common Vanilo functions, traits, helpers",
    "type": "library",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "license": "MIT",
    "keywords": ["ecommerce", "laravel", "vanilo"],
    "support": {
        "issues": "https://github.com/vanilophp/framework/issues",
        "source": "https://github.com/vanilophp/support"
    },
    "authors": [
        {
            "name": "Attila Fulop",
            "homepage": "https://github.com/fulopattila122"
        }
    ],
    "require": {
        "php": "^8.0",
        "illuminate/support": "^9.2",
        "nette/schema": "^1.2",
        "vanilo/contracts": "^3.6"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "Vanilo\\Support\\": ""
        }
    },
    "suggest": {
        "spatie/laravel-medialibrary": "Allows associating images or other files with Eloquent models (^10.0)",
        "konekt/concord": "Library to write Modular Applications (^1.11)"
    }
}

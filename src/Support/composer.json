{
    "name": "vanilo/support",
    "description": "Common Vanilo functions, traits, helpers",
    "type": "library",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "license": "MIT",
    "keywords": ["ecommerce", "laravel", "vanilo"],
    "support": {
        "issues": "https://github.com/vanilophp/framework/issues",
        "source": "https://github.com/vanilophp/support"
    },
    "authors": [
        {
            "name": "Attila Fulop",
            "homepage": "https://github.com/fulopattila122"
        }
    ],
    "require": {
        "php": "^8.3",
        "laravel/framework": "^10.48|^11.0|^12.0",
        "nette/schema": "^1.2",
        "vanilo/contracts": "^5.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0|^11.0",
        "illuminate/database": "^10.48|^11.0|^12.0"
    },
    "autoload": {
        "psr-4": {
            "Vanilo\\Support\\": ""
        }
    },
    "suggest": {
        "spatie/laravel-medialibrary": "Allows associating images or other files with Eloquent models (^11.0)",
        "konekt/concord": "Library to write Modular Applications (^1.13)",
        "konekt/address": "Improves the EU membership and VAT ID validation (^3.4)"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "5.1.x-dev"
        }
    }
}

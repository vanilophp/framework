{
    "name": "vanilo/shipment",
    "description": "Vanilo Shipment Module",
    "type": "library",
    "license": "MIT",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "keywords": ["shipping", "ecommerce", "vanilo", "laravel"],
    "support": {
        "issues": "https://github.com/vanilophp/framework/issues",
        "source": "https://github.com/vanilophp/shipment"
    },
    "authors": [
        {
            "name": "Attila Fulop",
            "homepage": "https://github.com/fulopattila122"
        }
    ],
    "require": {
        "php": "^8.3",
        "konekt/address": "^3.4.1",
        "konekt/concord": "^1.15",
        "konekt/enum": "^4.2",
        "konekt/laravel-migration-compatibility": "^1.6",
        "laravel/framework": "^10.48|^11.0|^12.0",
        "vanilo/contracts": "^5.0",
        "vanilo/support": "^5.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0|^11.0",
        "orchestra/testbench": "^8.0|^9.0|^10.0"
    },
    "autoload": {
        "psr-4": { "Vanilo\\Shipment\\": "" }
    },
    "suggest": {
        "vanilo/adjustments": "Allows storing shipping fees as adjustments for carts, orders, etc (^4.0)"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "5.0.x-dev"
        }
    }
}

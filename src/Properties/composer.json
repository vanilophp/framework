    "description": "Vanilo Properties (Attributes) Module",
    "type": "library",
    "license": "MIT",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "keywords": ["properties", "attributes", "eav", "ecommerce", "vanilo", "laravel"],
    "support": {
        "issues": "https://github.com/vanilophp/framework/issues",
